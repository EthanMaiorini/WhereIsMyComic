entity Characters {
	fullName String
    description String
    thumbnail ImageBlob
}

entity Series {
	name String
}

entity Comic {
    issueNumber Integer
	location String
	title String
	description String
	thumbnail ImageBlob
}


relationship OneToMany {
     Characters{series(characterId)} to Series
     Series{comic(seriesId)} to Comic
     Characters{comic(characterId)} to Comic
}
